{
  "name": "@cosmicvelocity/logging-js",
  "version": "0.0.6",
  "description": "It is a lightweight logging library.",
  "keywords": [
    "es6",
    "modules",
    "util"
  ],
  "author": "Kouichi Machida <k-machida@aideo.co.jp>",
  "license": "Apache-2.0",
  "private": false,
  "bugs": {
    "url": "https://github.com/cosmicvelocity/logging-js/issues"
  },
  "homepage": "https://github.com/cosmicvelocity/logging-js#readme",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "directories": {
    "lib": "build"
  },
  "scripts": {
    "build": "node ./node_modules/babel-cli/bin/babel.js ./src --out-dir build",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src",
    "test": "node ./node_modules/cross-env/dist/bin/cross-env.js NODE_ENV=test ./node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cosmicvelocity/logging-js.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jsdom": "^11.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "ie >= 11",
    "Chrome >= 21",
    "Firefox >= 28",
    "Safari >= 6.1",
    "Opera >= 12.1",
    "iOS >= 7.1",
    "Android >= 4.4"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "roots": [
      "<rootDir>/test"
    ],
    "testRegex": "/test/.*\\.js$",
    "verbose": true
  }
}